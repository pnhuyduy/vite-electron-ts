{
  "name": "vite-electron-ts",
  "version": "0.0.1",
  "description": "vite-electron-ts",
  "author": {
    "name": "cyyjs",
    "email": "cyyjs@cyyjs.top"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "vue",
    "electron"
  ],
  "main": "dist/main/index.cjs",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "node scripts/watch.mjs",
    "build": "node scripts/build.mjs && electron-builder"
  },
  "dependencies": {
    "naive-ui": "^2.29.0",
    "vue": "3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.2",
    "@vicons/antd": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.23",
    "electron": "^19.0.1",
    "electron-builder": "^22.14.5",
    "eslint": "^8.16.0",
    "typescript": "^4.5.2",
    "unplugin-auto-import": "^0.8.5",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.6.14",
    "vite-plugin-electron": "^0.4.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.4"
  },
  "build": {
    "productName": "vue-electron-ts",
    "appId": "vue.electron.ts",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "npmRebuild": false,
    "buildDependenciesFromSource": true,
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "files": [
      "dist/main/**/*",
      "dist/render/**/*",
      "dist/preload/**/*"
    ]
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000
  }
}
